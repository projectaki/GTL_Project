@model IEnumerable<GTL_Project.Models.ActiveLoan>

@{
    ViewBag.Title = "ShowPersonLoans";
}

<h2>Show Persons Loans</h2>

<table class="table">
    <tr>

        <th>
            @Html.DisplayNameFor(model => model.Ssn)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CopyId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Title)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LoanDate)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>

        <td>
            @Html.DisplayFor(modelItem => item.Ssn)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CopyId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Title)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LoanDate)
        </td>

    </tr>
}

</table>
